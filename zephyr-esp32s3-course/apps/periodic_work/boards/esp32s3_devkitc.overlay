/ {
    aliases {
        led0 = &onboard_led;
        led1 = &second_led;
    };

    leds {
        compatible = "gpio-leds";

        onboard_led: led_0 {
            gpios = <&gpio0 8 GPIO_ACTIVE_HIGH>;
            label = "GPIO-8 LED";
        };

        second_led: led_1 {
            gpios = <&gpio0 9 GPIO_ACTIVE_HIGH>;
            label = "GPIO-9 LED";
        };
    };
};
